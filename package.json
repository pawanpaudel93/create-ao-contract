    "create-ao-contract",
    "arweave",
    "ao"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "create-ao-contract": "dist/index.js"
  },
  "files": [
    "dist",
    "template",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json"
  ],
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "bun lint --fix",
    "format": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path .gitignore --write",
    "format:check": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path .gitignore --check",
    "release": "changeset version",
    "publish": "bun run build && changeset publish"
  },
  "dependencies": {
    "@clack/core": "^0.3.2",
    "@clack/prompts": "^0.6.3",
    "chalk": "5.2.0",
    "commander": "^10.0.0",
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "ora": "6.3.1",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.6",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.0",
    "@types/validate-npm-package-name": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "glob": "^10.3.10",
    "prettier": "^3.0.0",
    "tsup": "^6.7.0",
    "type-fest": "^3.7.0",
    "typescript": "^5.0.4"
  }
